		"npm": "^11.6.1",
		"ws": "^8.17.0"
	},
	"devDependencies": {
		"@types/node": "^20.11.5",
		"@types/ws": "^8.5.10",
		"nodemon": "^3.1.0",
		"pkg": "^5.8.1",
		"ts-node": "^10.9.2",
		"typescript": "^5.3.3"
	}
}
